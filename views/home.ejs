<%- include("partials/header") -%>


<h1>Home</h1>

<p> <%= content %></p>


<% posts.forEach(function(post) {
  var postPreview = post.content.substring(0, 100) + "...";%>
  <h1><%= post.title %></h1>
  <p style="display: inline"><%= postPreview %></p>
  <a href="/posts/<%= post.title %>"> Read More</a>
<%})%>


